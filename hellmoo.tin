#act {^The %1 treeman attacks you!$} 
{ 
    hb; pow 
}

#act {^%1 hacks a mossy bark off the body of %2 treeman.$} 
{
    put bark in trail
}

#act {^%1 (%2)   %3:%4{pm|am}%5$} 
{ 
    #var room_name %1; 
    #var area_name %2; 
}

#alias {maproom} 
{ 
    #map name $room_name; 
    #map set roomarea $area_name; 
    #showme Room name set to <160>$room_name<010> (<160>$area_name<010>) 
}

#highlight {^You shrug off the radiation sickness.$} {reverse}
#highlight {^From your wristpad: %1 pages, '%2'$} {reverse}
#highlight {burned-out} {reverse}

